   "source": [
    "# ======= SCALING NUMERICAL DATA ======== #"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 101,
   "id": "71b83d31-7f92-446b-b920-c9f0a6a4d177",
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.preprocessing import MinMaxScaler, OneHotEncoder\n",
    "\n",
    "scaler = MinMaxScaler().fit(inputs[numeric_cols])\n",
    "\n",
    "inputs[numeric_cols] = scaler.transform(inputs[numeric_cols])\n",
    "test_inputs[numeric_cols] = scaler.transform(test_inputs[numeric_cols])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "312b4508-00b1-4389-9388-7b7531d97709",
   "metadata": {},
   "outputs": [],
   "source": [
    "# ======== ENCODING CATEGORICAL DATA ======== #"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 115,
   "id": "60fc12b3-37e0-41f6-8439-4c624bad360d",
   "metadata": {},
   "outputs": [],
   "source": [
    "def encode_and_merge(encoder, df):\n",
    "    # encode categorical columns\n",
    "    encoded_cols = encoder.fit_transform(df[categorical_cols])\n",
    "\n",
    "    # convert to a dataframe\n",
    "    encoded_df = pd.DataFrame(\n",
    "        encoded_cols,\n",
    "        columns = encoder.get_feature_names_out(categorical_cols),\n",
    "        index = df.index,\n",
    "    )\n",
    "\n",
    "    # merge categorical and numeric columns\n",
    "    merged_cols = pd.concat([df.drop(columns=categorical_cols), encoded_df], axis=1)\n",
    "    return merged_cols"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 116,
   "id": "2c75f67f-d5db-4c4c-a89e-85b08d23e03e",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Convert values in categorical columns to strings\n",
    "inputs[categorical_cols] = inputs[categorical_cols].astype(str)\n",
    "test_inputs[categorical_cols] = test_inputs[categorical_cols].astype(str)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 117,
   "id": "43fc19b5-f8fa-4040-90d7-bb6a491a16f0",
   "metadata": {},
   "outputs": [],
   "source": [
    "encoder = OneHotEncoder(handle_unknown='ignore', sparse_output=False).fit(inputs[categorical_cols])\n",
    "\n",
    "X_train = encode_and_merge(encoder, inputs)\n",
    "X_test = encode_and_merge(encoder, test_inputs)"
   ]
